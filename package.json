{
  "name": "@algonaut/algo-validation-agent",
  "version": "0.1.0",
  "description": "Client validation library for Algorand transaction inputs",
  "main": "dist/rollup-plugin-babel.cjs.js",
  "module": "dist/rollup-plugin-babel.esm.js",
  "browser": "dist/rollup-plugin-babel.iife.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "docs:generate": "vuepress-jsdoc --source ./lib --dist ./docs --folder api --title Algo Validation Agent --exclude *.test.ts,*.d.ts,exclude.js",
    "docs:dev": "vuepress dev ./docs",
    "docs:deploy": "vuepress build ./docs",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline && rollup -c",
    "build": "npm run build:types && npm run build:js",
    "build:all": "npm run test --collectCoverage && npm run build && npm run docs:generate"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algonaut/algo-validation-agent.git"
  },
  "keywords": [
    "algorand",
    "typescript",
    "validation"
  ],
  "author": "@jciruolo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/algonaut/algo-validation-agent/issues"
  },
  "homepage": "https://github.com/algonaut/algo-validation-agent#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.0",
    "babel-jest": "^25.3.0",
    "jest": "^25.3.0",
    "rollup": "^2.4.0",
    "rollup-plugin-babel": "^4.4.0",
    "typescript": "^3.8.3",
    "vuepress": "^1.4.0",
    "vuepress-jsdoc": "^2.1.1"
  },
  "dependencies": {
    "v8n": "^1.3.3"
  }
}
